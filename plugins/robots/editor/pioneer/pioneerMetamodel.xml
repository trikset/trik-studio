						<image y1="0" name="images/getVelocityBlock.svg" x1="0" y2="50" x2="50"/>
					</picture>
					<labels>
						<label x="35" y="60" textBinded="X" background="white" prefix="X:"/>
						<label x="35" y="90" textBinded="Y" background="white" prefix="Y:"/>
